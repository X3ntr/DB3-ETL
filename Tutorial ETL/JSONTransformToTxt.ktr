      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here
var appID = 0
 
//maak van de jsonString een JSON object zodat we door het object kunnen navigeren
jsonObject = JSON.parse(jsonString)
 
// Haal de array van Apps uit het JSON object
apps = jsonObject.Apps
 
// Voer voor elk element uit de apps array de functie appLoop uit
apps.forEach(appLoop)
 
 
function appLoop(currentApp){
	appID = currentApp.ID
	
//Voer voor elk element uit de array MajorReleases de majorReleaseLoop uit
currentApp.MajorReleases.forEach(majorReleaseLoop)
	
}	
 
function majorReleaseLoop(majorRelease){
// Je zou nu gewoon de warden op de outputvelden van de step kunnen zetten, maar we willen voor elke Major Release een rij maken. Daarom voegen we voor elke major release een rij toe. We nemen eerst een kopie van de huidige rij.
	var row = createRowCopy(getOutputRowMeta().size());
// In dit geval zullen er op de rij 4 kolommen zijn: 2 inputvelden en twee outputvelden
	var idx = getInputRowMeta().size();
	// We plaatsen appID en majorRelease respectievelijk in de 3de en 4de kolom.
row[idx++] = appID
	row[idx++] = majorRelease
// putRow zet de rij op de outputStream
	putRow(row)
}

// Omdat we geen waarden hebben toegekend aan de variabelen wordt dit door Pentaho als fout aanzien. Daarom moet je expliciet zeggen dat je de transformatie voor deze rij wil overslaan. In werkelijkheid heb je natuurlijk eerst wel al X aantal rijen op de output gezet.
trans_Status = SKIP_TRANSFORMATION;

</jsScript_script>
